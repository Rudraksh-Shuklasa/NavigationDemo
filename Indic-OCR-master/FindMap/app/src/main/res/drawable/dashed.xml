
<rotate xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="90"
    android:toDegrees="90">
    <shape
        android:shape="line">
        <stroke
            android:width="1dp"
            android:color="#A1000000"

            android:dashWidth="8dp"
            android:dashGap="2dp" />
        <size android:width="120dp" />
    </shape>
</rotate>